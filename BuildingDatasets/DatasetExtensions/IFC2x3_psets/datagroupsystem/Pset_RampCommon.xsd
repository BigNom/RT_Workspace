<?xml version="1.0" encoding="Windows-1252"?>
<xs:schema elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:simpleType name="bWorkingUnitsType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:element name="Pset_RampCommon">
		<xs:complexType>
			<xs:attribute name="Reference" type="xs:string" use="optional" default=""/>
			<xs:attribute name="RequiredHeadroom" type="bWorkingUnitsType" use="optional"/>
			<xs:attribute name="RequiredSlope" type="xs:decimal" use="optional" default="0"/>
			<xs:attribute name="IsExternal" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="FireRating" type="xs:string" use="optional" default=""/>
			<xs:attribute name="FireExit" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="HandicapAccessible" type="xs:boolean" use="optional" default="false"/>
			<xs:attribute name="HasNonSkidSurface" type="xs:boolean" use="optional" default="false"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
